{
  "name": "GLIB-ChromeExt",
  "version": "1.0",
  "description": "GLIB-ChromeExt",
  "permissions": [
    "https://glib-mock.herokuapp.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "lib/vex/vex.css",
        "lib/vex/vex-theme-os.css"
      ],
      "js": [
        "config.js",
        "lib/axios.js",
        "lib/async.js",
        "lib/oauth.js",
        "lib/jquery.js",
        "lib/vex/vex.combined.min.js",
        "src/content_script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    // "lib/vex/vex.css",
    // "lib/vex/vex-theme-os.css"
  ]
}
